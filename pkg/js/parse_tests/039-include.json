{
  "dns_providers": [
    {
      "name": "Cloudflare",
      "type": "CLOUDFLAREAPI"
    }
  ],
  "domains": [
    {
      "dnsProviders": {
        "Cloudflare": -1
      },
      "name": "foo.com!external",
      "records": [
        {
          "name": "@",
          "target": "1.2.3.4",
          "type": "A"
        }
      ],
      "registrar": "Third-Party"
    },
    {
      "dnsProviders": {
        "Cloudflare": -1
      },
      "name": "foo.com!internal",
      "records": [
        {
          "name": "@",
          "target": "1.2.3.4",
          "type": "A"
        },
        {
          "name": "local",
          "target": "127.0.0.1",
          "type": "A"
        }
      ],
      "registrar": "Third-Party"
    }
  ],
  "registrars": [
    {
      "name": "Third-Party",
      "type": "NONE"
    }
  ]
}
