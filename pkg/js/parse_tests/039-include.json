{
  "registrars": [
    {
      "name": "Third-Party",
      "type": "NONE"
    }
  ],
  "dns_providers": [
    {
      "name": "Cloudflare",
      "type": "CLOUDFLAREAPI"
    }
  ],
  "domains": [
    {
      "name": "foo.com!external",
      "registrar": "Third-Party",
      "dnsProviders": {
        "Cloudflare": -1
      },
      "records": [
        {
          "type": "A",
          "name": "@",
          "origin": "",
          "target": "1.2.3.4"
        }
      ]
    },
    {
      "name": "foo.com!internal",
      "registrar": "Third-Party",
      "dnsProviders": {
        "Cloudflare": -1
      },
      "records": [
        {
          "type": "A",
          "name": "@",
          "origin": "",
          "target": "1.2.3.4"
        },
        {
          "type": "A",
          "name": "local",
          "origin": "",
          "target": "127.0.0.1"
        }
      ]
    }
  ]
}