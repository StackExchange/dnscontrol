{
  "registrars": [
    {
      "name": "Third-Party",
      "type": "NONE"
    }
  ],
  "dns_providers": [
    {
      "name": "otherconfig",
      "type": "CLOUDFLAREAPI"
    },
    {
      "name": "Cloudflare",
      "type": "CLOUDFLAREAPI"
    },
    {
      "name": "bind",
      "type": "BIND"
    }
  ],
  "domains": [
    {
      "name": "example.com",
      "registrar": "Third-Party",
      "dnsProviders": {
        "otherconfig": -1
      },
      "records": [
        {
          "type": "A",
          "name": "main",
          "target": "3.3.3.3"
        },
        {
          "type": "A",
          "name": "www",
          "target": "33.33.33.33"
        }
      ]
    },
    {
      "name": "example.com!inside",
      "registrar": "Third-Party",
      "dnsProviders": {
        "Cloudflare": -1
      },
      "records": [
        {
          "type": "A",
          "name": "main",
          "target": "1.1.1.1"
        },
        {
          "type": "A",
          "name": "main",
          "target": "11.11.11.11"
        }
      ]
    },
    {
      "name": "example.com!outside",
      "registrar": "Third-Party",
      "dnsProviders": {
        "bind": -1
      },
      "records": [
        {
          "type": "A",
          "name": "main",
          "target": "8.8.8.8"
        }
      ]
    }
  ]
}