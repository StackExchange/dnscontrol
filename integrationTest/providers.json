{
    "BIND": {
        "domain": "example.com"
    },
    "DNSIMPLE": {
        //16/17: no ns records managable. Not even for subdomains.
        "knownFailures": "17,18",
        "domain": "$DNSIMPLE_DOMAIN",
        "token": "$DNSIMPLE_TOKEN",
        "baseurl": "https://api.sandbox.dnsimple.com"
    },
    "DYN": {
        "domain": "$DYN_DOMAIN",
        "customer": "$DYN_CUSTOMER",
        "username":"$DYN_USER",
        "password": "$DYN_PW"
    },
    "GANDI":{
        //5: gandi does not accept ttls less than 300
        "knownFailures": "5",
        "domain": "$GANDI_DOMAIN",
        "apikey": "$GANDI_KEY"
    },
    "GCLOUD": {
        "domain": "$GCLOUD_DOMAIN",
        "project_id": "$GCLOUD_PROJECT",
        "private_key": "$GCLOUD_PRIVATEKEY",
        "client_email": "$GCLOUD_EMAIL"
    },
    "ROUTE53": {
        "domain": "$R53_DOMAIN",
        "KeyId": "$R53_KEY_ID",
        "SecretKey": "$R53_KEY"
    },
    "ACTIVEDIRECTORY_PS":{
        "knownFailures": "17,18,19,25,26,27,28,29,30",
        "domain": "$AD_DOMAIN",
        "ADServer": "$AD_SERVER"
    }
}