  "NS1": {
    "domain": "$NS1_DOMAIN",
    "api_token": "$NS1_TOKEN"
  },
  "NAMEDOTCOM": {
    "apikey": "$NAMEDOTCOM_KEY",
    "apiurl": "$NAMEDOTCOM_URL",
